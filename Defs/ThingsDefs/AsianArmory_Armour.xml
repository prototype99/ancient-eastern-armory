	<!-- Samurai Yoroi -->
	<ThingDef ParentName="ArmorSmithableBase">
		<defName>Ancient_SamuraiYoroi</defName>
		<label>Samurai Yoroi</label>
		<description>Sets of Heavy armor worn by Samurai class of feudal japan. It has slightly lower defense than plate armor, but has higher flexibility.</description>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
			<researchPrerequisite>PlateArmor</researchPrerequisite>
			<recipeUsers>
				<li>ElectricSmithy</li>
				<li>FueledSmithy</li>
			</recipeUsers>
			<skillRequirements>
				<Crafting>10</Crafting>
			</skillRequirements>
		</recipeMaker>
		<graphicData>
			<texPath>Armor/SamuraiYoroi</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>200</costStuffCount>
		<statBases>
			<WorkToMake>41000</WorkToMake>
			<MaxHitPoints>270</MaxHitPoints>
			<Mass>13</Mass>
			<StuffEffectMultiplierArmor>0.8</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>1.0</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>17</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.65</MoveSpeed>
		</equippedStatOffsets>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Hands</li>
				<li>Legs</li>
				<li>Feet</li>
			</bodyPartGroups>
			<wornGraphicPath>Armor/SamuraiYoroi</wornGraphicPath>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<tags>
				<li>MedievalMilitary</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<renderSkipFlags><li>None</li></renderSkipFlags>
			<drawData>
				<dataNorth>
					<layer>51</layer>
				</dataNorth>
			</drawData>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(255,255,255,255)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>
